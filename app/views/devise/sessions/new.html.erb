<header class="landing-container">


  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

    <%= f.email_field :email, autofocus: true, placeholder: "user@example.com" %>
    <%= f.password_field :password, autocomplete: "off" %>
    <%= f.submit "Log in", class:'btn btn-lg btn-primary' %> <%= link_to "Sign Up here", new_user_registration_path, class:'signup-buttn' %>


<% end %>


</header>
    

